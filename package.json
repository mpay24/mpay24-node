{
  "name": "mpay24-node",
  "version": "0.0.4",
  "description": "mPAY24 SDK",
  "main": "lib/mpay24.js",
  "scripts": {
    "build": "babel -d ./lib src",
    "prepublish": "npm run build",
    "test": "xo && npm run build && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpay24/mpay24-node.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".*": "./node_modules/async-to-gen"
    },
    "setupTestFrameworkScriptFile": "./jenkinsreporter.js"
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "space": true,
    "rules": {
      "comma-dangle": [
        "error",
        "always"
      ],
      "new-cap": [
        "error",
        {
          "newIsCap": false
        }
      ]
    },
    "global": [
      "expect",
      "beforeAll",
      "jasmine",
      "jest"
    ],
    "env": [
      "node",
      "mocha"
    ]
  },
  "author": "Tobias Lins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpay24/mpay24-node/issues"
  },
  "homepage": "https://github.com/mpay24/mpay24-node#readme",
  "dependencies": {
    "es6-promise": "^4.0.5",
    "js2xmlparser": "^2.0.2",
    "soap": "^0.17.0"
  },
  "devDependencies": {
    "async-to-gen": "^1.1.4",
    "babel-cli": "^6.16.0",
    "babel-jest": "^17.0.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "jasmine-reporters": "^2.2.0",
    "jest": "^17.0.2",
    "xo": "^0.17.0"
  }
}
