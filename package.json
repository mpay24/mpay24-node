{
  "name": "mpay24-node",
  "version": "0.0.2",
  "description": "mPAY24 SDK",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d ./lib src",
    "prepublish": "npm run build",
    "test": "xo && npm run build && ava --verbose --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpay24/mpay24-node.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "space": true,
    "rules": {
      "comma-dangle": ["error", "always"],
      "new-cap": ["error", { "newIsCap": false }]
    },
    "env": [
      "node",
      "mocha"
    ]
  },
  "author": "Tobias Lins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpay24/mpay24-node/issues"
  },
  "homepage": "https://github.com/mpay24/mpay24-node#readme",
  "dependencies": {
    "es6-promise": "^4.0.5",
    "js2xmlparser": "^2.0.2",
    "soap": "^0.17.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "xo": "^0.17.0"
  }
}
